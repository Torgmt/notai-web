{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "pnpmVersion": "9.12.2",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "buildCommand": "pnpm run vercel:info && pnpm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://designs-properties-affordable-graduated.trycloudflare.com/$1"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}


